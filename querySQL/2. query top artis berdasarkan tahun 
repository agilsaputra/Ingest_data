SELECT
  `ARTIST CLEAN` AS `Artist Name`,
  COUNT(CASE WHEN `Release Year` = 1970 THEN 1 END) AS `1970`,
  COUNT(CASE WHEN `Release Year` = 1971 THEN 1 END) AS `1971`,
  COUNT(CASE WHEN `Release Year` = 1972 THEN 1 END) AS `1972`,
  COUNT(CASE WHEN `Release Year` = 1973 THEN 1 END) AS `1973`,
  COUNT(CASE WHEN `Release Year` = 1974 THEN 1 END) AS `1974`,
   COUNT(CASE WHEN `Release Year` = 1975 THEN 1 END) AS `1975`
FROM
  test_table 
GROUP BY
  `ARTIST CLEAN`
 ORDER BY
  COUNT(*) DESC;
